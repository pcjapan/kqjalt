{% for p in page.collection %}
<div class="m-2">
<div class="heading-date m-1">{{ p.header.eventdate|date("j F, Y") }}</div>
<div class="heading-time m-1">{{ p.header.eventstart|date("g:i a") }} - {{ p.header.eventend|date("g:i a") }}</div>
<div class="heading-title m-1"><a href="{{ p.url }}"> {{ p.title }} </a></div>
<div class="heading-speaker m-1">{{ p.header.eventspeaker }}</div>
<hr>
</div>
{% endfor %}


